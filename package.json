{
  "name": "react-setup",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@hookform/resolvers": "^2.6.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^13.2.1",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "dayjs": "^1.10.6",
    "eslint-plugin-import": "^2.23.4",
    "i18next": "^20.3.5",
    "i18next-browser-languagedetector": "^6.1.2",
    "is-ci-cli": "^2.2.0",
    "jwt-decode": "^3.1.2",
    "node-sass": "^6.0.1",
    "pretty-checkbox": "^3.0.3",
    "pretty-checkbox-react": "^3.2.0",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.1",
    "react-bootstrap-typeahead": "^5.1.4",
    "react-country-flag": "^2.3.1",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.10.1",
    "react-hot-toast": "^2.0.0",
    "react-i18next": "^11.11.3",
    "react-js-pagination": "^3.0.3",
    "react-pro-sidebar": "^0.6.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "react-slick": "^0.28.1",
    "react-tooltip": "^4.2.21",
    "redux": "^4.1.0",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "slick-carousel": "^1.8.1",
    "source-map-explorer": "^2.5.2",
    "yup": "^0.32.9"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "is-ci test:coverage test:watch",
    "test:watch": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:related": "react-scripts test --findRelatedTests --watchAll=false",
    "eject": "react-scripts eject",
    "staging": "REACT_APP_ENV=staging npm start",
    "production": "REACT_APP_ENV=production npm start",
    "build:staging": "REACT_APP_ENV=staging npm run build",
    "build:production": "REACT_APP_ENV=production npm run build",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^7.0.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-jest": "^24.4.2",
    "file-loader": "^6.2.0",
    "husky": "^7.0.2",
    "jest-watch-typeahead": "^0.6.4",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,test.js,test.ts}": [
      "prettier --write",
      "eslint --fix",
      "npm run test:related"
    ]
  }
}
